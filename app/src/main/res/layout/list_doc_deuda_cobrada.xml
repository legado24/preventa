 <androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:focusable="false" android:layout_margin="5sp"
    android:focusableInTouchMode="false"
    android:clickable="true"
    card_view:cardUseCompatPadding="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <androidx.core.widget.NestedScrollView
            android:id="@+id/nested_scroll_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:scrollbars="none"
            android:scrollingCache="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="true"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:padding="@dimen/spacing_middle">



                        <View
                            android:layout_width="@dimen/spacing_middle"
                            android:layout_height="0dp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">
                                <LinearLayout android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"  android:layout_weight="2">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/txtSerie"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/spacing_medium"
                                            android:text="F0001"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                            android:textColor="@color/main_color_grey_900"
                                            app:fontFamily="sans-serif-medium" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/spacing_medium"
                                            android:text=" - "
                                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                            android:textColor="@color/main_color_grey_900"
                                            app:fontFamily="sans-serif-medium" />

                                        <TextView
                                            android:id="@+id/txtPreImpreso"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/spacing_medium"
                                            android:text="0000001"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                            android:textColor="@color/main_color_grey_900"
                                            app:fontFamily="sans-serif-medium" />
                                    </LinearLayout>

                                    <LinearLayout android:layout_weight="1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content" android:orientation="horizontal">

                                        <TextView android:id="@+id/txtFechaEmision"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/spacing_medium"
                                            android:layout_weight="1"

                                            android:text="2019-12-12"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Subhead"
                                            android:textColor="@color/main_color_grey_900"
                                            app:fontFamily="sans-serif-medium" />

                                    </LinearLayout>


                                </LinearLayout>


                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content" android:orientation="horizontal">
                                    <TextView android:id="@+id/txtCodVendedor"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="006"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                        android:textColor="@color/main_color_grey_900" />
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text=" - "
                                        android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                        android:textColor="@color/main_color_grey_900" />
                                    <TextView android:id="@+id/txtDescVendedor"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="VENDEDOR 1"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                        android:textColor="@color/main_color_grey_900" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content" android:orientation="horizontal">

                                    <LinearLayout android:layout_weight="1"  android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal">
                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"   android:layout_marginTop="@dimen/spacing_small"
                                            android:text="MONTO:"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                            android:textColor="@color/main_color_grey_900" />
                                        <TextView android:id="@+id/txtMonto"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/spacing_small"
                                            android:text="$ 89.00"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                            android:textColor="@color/main_color_grey_900"
                                            android:textStyle="bold" />
                                    </LinearLayout>

                                    <LinearLayout android:layout_weight="1"  android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal">
                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"   android:layout_marginTop="@dimen/spacing_small"
                                            android:text="SALDO:"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                            android:textColor="@color/main_color_grey_900" />
                                        <TextView android:id="@+id/txtSaldo"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="@dimen/spacing_small"
                                            android:text="$ 89.00"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                            android:textColor="@color/main_color_grey_900"
                                            android:textStyle="bold" />
                                    </LinearLayout>


                                </LinearLayout>
                                <View
                                    android:layout_width="@dimen/spacing_middle"
                                    android:layout_height="0dp" />



                                <LinearLayout android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">
                                    <TextView   android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                        android:textColor="@color/main_color_grey_900" android:text="ESTADO:"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content" />

                                    <TextView
                                        android:id="@+id/txtEstado" android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                        android:textColor="@color/main_color_grey_900"
                                        android:layout_width="wrap_content" android:text="VENCIDO"
                                        android:layout_height="wrap_content"     android:textSize="@dimen/sizeFormItem" />
                                </LinearLayout>

                                <View
                                    android:layout_width="@dimen/spacing_middle"
                                    android:layout_height="0dp" />

                                <LinearLayout android:orientation="horizontal"
                                    android:layout_width="match_parent" android:weightSum="2"
                                    android:layout_height="wrap_content">
                                    <LinearLayout android:orientation="horizontal" android:layout_weight="1"
                                        android:layout_width="match_parent" android:gravity="center"
                                        android:layout_height="match_parent">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:layout_gravity="center"
                                            android:gravity="center|left"
                                            android:text="MONTO COBRADO="
                                            android:textColor="@color/colorPrimary" />
                                    </LinearLayout>
                                    <LinearLayout android:orientation="horizontal" android:gravity="center"
                                        android:layout_width="match_parent" android:layout_gravity="center" android:layout_weight="1"
                                        android:layout_height="wrap_content">

                                        <EditText
                                            android:id="@+id/txtMontoPagado"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="bottom"
                                            android:inputType="numberDecimal"
                                            android:textColor="@color/amber900"
                                            android:theme="@style/customtheme" />
                                    </LinearLayout>


                                </LinearLayout>



                                            <LinearLayout android:orientation="horizontal" android:layout_weight="2"
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent">
                                                <Button android:theme="@style/RaisedButtonDark" android:layout_weight="1" android:drawableRight="@drawable/deleteitem"
                                                    android:layout_width="match_parent" android:text="Eliminar"
                                                    android:layout_height="wrap_content" android:id="@+id/btnEliminar"/>

                                                <Button android:theme="@style/RaisedButtonDark" android:text="Editar"  android:layout_weight="1" android:drawableRight="@drawable/edititem"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content" android:id="@+id/btnEditar"/>

                                            </LinearLayout>





                                <View
                                    android:layout_width="0dp"
                                    android:layout_height="0dp"
                                    android:layout_weight="1" />


                            </LinearLayout>




                        </LinearLayout>


                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/main_color_grey_400" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/spacing_mlarge" />

                </LinearLayout>

            </LinearLayout>
        </androidx.core.widget.NestedScrollView>





    </LinearLayout>

</androidx.cardview.widget.CardView>